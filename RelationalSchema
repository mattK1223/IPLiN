CREATE TABLE Owner (
  ownerName VARCHAR(50) PRIMARY KEY,
  addr VARCHAR(200)
);
  
CREATE TABLE ip (
  ownerName VARCHAR(50) NOT NULL,
  url VARCHAR(200) PRIMARY KEY,
  title VARCHAR(200),
  FOREIGN KEY(ownerName) REFERENCES Owner,
    ON DELETE CASCADE
);
  
CREATE TABLE patent (
  url VARCHAR(200) PRIMARY KEY, 
  appNum INT UNIQUE, 
  patentNum INT UNIQUE,
  filingDate DATE,
  issueDate DATE,
  abstract VARCHAR(1000),
  FOREIGN KEY(url) REFERENCES ip,
    ON DELETE CASCADE
);

CREATE TABLE trademark (
  url VARCHAR(200) PRIMARY KEY,
  serialNum INT UNIQUE,
  regNum INT UNIQUE,
  status CHAR(4),
  filingDate DATE,
  FOREIGN KEY(url) REFERENCES ip,
    ON DELETE CASCADE
);

CREATE TABLE copyright (
  url VARCHAR(200) PRIMARY KEY,
  registrationNum INT,
  publicationDate DATE,
  registrationDate DATE,
  type VARCHAR(100),
  FOREIGN KEY(url) REFERENCES ip,
    ON DELETE CASCADE
);

CREATE TABLE video (
  url VARCHAR(200) PRIMARY KEY, 
  channelURL VARCHAR(200),
  publicationDate DATE,
  FOREIGN KEY(url) REFERENCES ip,
    ON DELETE CASCADE
);

CREATE TABLE License (
  licensee VARCHAR(100) NOT NULL,
  ownerName VARCHAR(50) NOT NULL, 
  url VARCHAR(100) NOT NULL,
  startDate DATE,
  endDate DATE,
  payment REAL,
  PRIMARY KEY(url, licensee, ownerName),
  FOREIGN KEY(url) REFERENCES ip,
  FOREIGN KEY(ownerName) REFERENCES Owner
);
 
